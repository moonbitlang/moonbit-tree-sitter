{
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "target": "ES2023",
    "outDir": "../dist",
    "lib": ["ES2023", "DOM"],
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true
  },
  "references": [{ "path": "sidebar" }]
}
