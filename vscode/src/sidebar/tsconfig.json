{
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "target": "ES2022",
    "outDir": "../../dist/sidebar",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "bundler",
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true,
    "noImplicitAny": false,
    "jsx": "react",
    "esModuleInterop": true
  }
}
